{
    "window.zoomLevel": 0,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "[ruby]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 4
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "terminal.external.osxExec": "iTerm.app",
    "diffEditor.ignoreTrimWhitespace": false
}
